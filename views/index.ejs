<body>
  <div class="title">
  <h1>Shopping+</h1>
</div>
<div class="list1">
<h2>List 1</h2>
</div>
  <ul>
    <% for(let i = 0; i < shoppings.length; i++) { %>
      <% if(shoppings[i].alreadyGot) { %>
        <strike><a class ="list" href="/shopping/<%=shoppings[i]._id%>">
          <%=shoppings[i].name%></strike>
     <% } else {%>
      <a class ="list" href="/shopping/<%=shoppings[i]._id%>">
        <%=shoppings[i].name%>
    <% } %>
    <% } %>
  </ul>
<div class="create">
  <button><a href="/shopping/new">Add Item</a></button>
</div>
</body>
</html>